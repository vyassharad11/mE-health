<?xml version="1.0" encoding="utf-8"?>
<com.mE.Health.utility.roundview.RoundLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginRight="@dimen/dp_5"
    android:paddingHorizontal="@dimen/dp_10"
    android:paddingVertical="@dimen/dp_8"
    android:layout_marginTop="@dimen/dp_10"
    app:rv_backgroundColor="@color/white"
    app:rv_strokeWidth="@dimen/dp_1"
    android:gravity="center"
    android:layout_gravity="center"
    app:rv_strokeColor="@color/text_color_orange"
    app:rv_isRadiusHalfHeight="true">

    <TextView
        android:id="@+id/tvItemName"
        style="@style/textStyleMedium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:text="Cancelled"
        android:textColor="@color/text_color_orange"
        android:textSize="@dimen/sp_12" />

    <ImageView
        android:id="@+id/ivChecked"
        android:layout_width="@dimen/dp_15"
        android:layout_height="@dimen/dp_15"
        android:layout_marginStart="@dimen/dp_5"
        android:src="@drawable/ic_tick_orange" />

</com.mE.Health.utility.roundview.RoundLinearLayout>